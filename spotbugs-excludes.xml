<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <!-- This plugin handles file locations provided by user input. -->
  <Match>
    <Class name="gov.va.plugin.maven.swagger.SwaggerMojo" />
    <Bug pattern="PATH_TRAVERSAL_IN" />
  </Match>
  <Match>
    <Class name="gov.va.plugin.maven.swagger.ExampleInjector" />
    <Bug pattern="PATH_TRAVERSAL_IN" />
  </Match>
</FindBugsFilter>
